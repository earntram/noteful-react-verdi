{
  "version": 2,
  "name": "noteful-react-verdi",
  "alias": "noteful-react-verdi",
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
  ],
  "env" : {
    "NOTEFUL_PROD_API_URL": "https://noteful-api-verdi.herokuapp.com/api"
  }
}